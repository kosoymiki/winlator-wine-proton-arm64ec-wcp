+    private String buildRequestedUpscalerModules() {
+        ArrayList<String> modules = new ArrayList<>();
+        if (!UPSCALE_VKBASALT_NONE.equals(upscalerVkbasaltEffect)) modules.add("vkbasalt");
+        if (!UPSCALE_SCREENFX_OFF.equals(upscalerScreenFxPipeline)) modules.add("screenfx");
+        if (!UPSCALE_SCALEFORCE_OFF.equals(upscalerScaleForcePreset)) modules.add("scaleforce");
+        if (!UPSCALE_PROTON_FSR_OFF.equals(upscalerProtonFsrMode)) modules.add("proton_fsr");
+        if (isDx8AssistRequested()) modules.add("dx8assist");
+        if (modules.isEmpty()) return "none";
+
+        StringBuilder sb = new StringBuilder();
+        for (int i = 0; i < modules.size(); i++) {
+            if (i > 0) sb.append(',');
+            sb.append(modules.get(i));
+        }
+        return sb.toString();
+    }
+
