# Runtime bundle lock (audit/enforce) for the glibc group shipped inside:
#   lib/wine/wcp-glibc-runtime/
#
# This lock is intentionally regex-based so host-mode builds can be audited
# without hardcoding per-distribution package strings too tightly.

WCP_RUNTIME_LOCK_ID="glibc-2.43-bundle-v1"

# glibc 2.43 target line (libc/loader). In host mode this will usually mismatch
# until pinned-source runtime is supplied; the verifier reports audit mismatches.
WCP_LOCK_EXPECT_LIBC_REGEX='(GLIBC[^0-9]*2\.43|GNU C Library.*2\.43|Ubuntu GLIBC 2\.43)'
WCP_LOCK_EXPECT_LOADER_REGEX='(GLIBC[^0-9]*2\.43|GNU C Library.*2\.43|Ubuntu GLIBC 2\.43)'

# Adjacent runtime libs are intentionally looser for v1. These will be tightened
# after the first pinned runtime bundle line is generated and fingerprinted.
WCP_LOCK_EXPECT_LIBSTDCXX_REGEX='(GLIBCXX_|GCC: \()'
WCP_LOCK_EXPECT_LIBGCC_REGEX='(GCC_|GCC: \()'
WCP_LOCK_EXPECT_LIBSDL2_REGEX='(SDL[_ ]?2(\.[0-9]+)+|UNKNOWN|MISSING)'

